{
    "name": "pos",
    "license": "MIT",
    "description": "pos description",
    "author": {
        "name": "Soufiane Mghanen",
        "email": "mgh.soufiane@gmail.com"
    },
    "build": {
        "appId": "com.pos"
    },
    "main": "dist/electron.js",
    "homepage": "http://localhost/",
    "version": "0.1.0",
    "private": false,
    "dependencies": {
        "bootstrap": "^4.3.1",
        "connected-react-router": "^6.4.0",
        "history": "^4.9.0",
        "jquery": ">=3.4.0",
        "nedb": "^1.8.0",
        "popper.js": "^1.14.7",
        "react": "^16.8.6",
        "react-bootstrap": "^1.0.0-beta.9",
        "react-dom": "^16.8.6",
        "react-redux": "^6.0.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0"
    },
    "scripts": {
        "test": "cross-env NODE_ENV=test node --trace-warnings scripts/test.js",
        "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 server.js",
        "dev": "npm run hot-server -- --start-hot",
        "react-start": "node scripts/start.js",
        "react-build": "node scripts/build.js",
        "electron-build": "electron-builder",
        "release": "yarn react-build && electron-builder --publish=always",
        "build": "yarn react-build && yarn electron-build",
        "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"tsc -w\" \"wait-on http://localhost:3000 && electron .\""
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.4.3",
        "@babel/plugin-transform-react-jsx-self": "^7.2.0",
        "@svgr/webpack": "4.1.0",
        "@types/electron": "^1.6.10",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/history": "^4.7.2",
        "@types/jest": "24.0.13",
        "@types/nedb": "^1.8.7",
        "@types/node": "^12.0.8",
        "@types/react": "^16.8.19",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.0.9",
        "@types/react-router": "^5.0.1",
        "@types/react-router-dom": "^4.3.3",
        "@types/redux-logger": "^3.0.7",
        "@types/webpack-env": "^1.13.9",
        "@typescript-eslint/eslint-plugin": "^1.10.2",
        "@typescript-eslint/parser": "^1.10.2",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.8.0",
        "babel-loader": "8.0.5",
        "babel-plugin-named-asset-import": "^0.3.2",
        "babel-preset-react-app": "^9.0.0",
        "camelcase": "^5.2.0",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "concurrently": "^4.1.0",
        "cross-env": "^5.2.0",
        "css-loader": "2.1.1",
        "dotenv": "6.2.0",
        "dotenv-expand": "4.2.0",
        "electron": "^5.0.2",
        "electron-builder": "^20.43.0",
        "electron-devtools-installer": "^2.2.4",
        "electron-is-dev": "^1.1.0",
        "electron-reload": "^1.4.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-config-react-app": "^4.0.1",
        "eslint-loader": "2.1.2",
        "eslint-plugin-flowtype": "2.x",
        "eslint-plugin-import": "2.x",
        "eslint-plugin-jsx-a11y": "6.x",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "7.x",
        "eslint-plugin-react-hooks": "1.5.0",
        "file-loader": "3.0.1",
        "fs-extra": "7.0.1",
        "html-webpack-plugin": "4.0.0-beta.5",
        "identity-obj-proxy": "3.0.0",
        "is-wsl": "^1.1.0",
        "jest": "24.7.1",
        "jest-environment-jsdom-fourteen": "0.1.0",
        "jest-resolve": "24.7.1",
        "jest-watch-typeahead": "0.3.0",
        "mini-css-extract-plugin": "0.5.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "pnp-webpack-plugin": "1.2.1",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "7.0.1",
        "postcss-preset-env": "6.6.0",
        "postcss-safe-parser": "4.0.1",
        "prettier": "^1.18.2",
        "react-app-polyfill": "^1.0.1",
        "react-dev-utils": "^11.0.4",
        "react-hot-loader": "^4.9.0",
        "redux-logger": "^3.0.6",
        "resolve": "1.10.0",
        "sass-loader": "7.1.0",
        "semver": "6.0.0",
        "style-loader": "0.23.1",
        "terser-webpack-plugin": "1.2.3",
        "ts-pnp": "1.1.2",
        "typescript": "3.4.5",
        "url-loader": "1.1.2",
        "wait-on": "^3.2.0",
        "webpack": "4.29.6",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "3.2.1",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-manifest-plugin": "2.0.4",
        "webpack-merge": "^4.2.1",
        "workbox-webpack-plugin": "4.2.0"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
            "\\.(css|less|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "modulePaths": [
            "node_modules"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
